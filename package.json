{
  "name": "photos",
  "version": "0.0.1",
  "private": true,
  "homepage": "https://photos.daneden.me",
  "engines": {
    "node": ">=10.8.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
